<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.junmeng.service.alpha.persistence.sigma.UserMapper" >

    <resultMap id="BaseResultMap" type="com.junmeng.model.alpha.persistence.sigma.User">
        <id column="id"                         property="id"                   jdbcType="INTEGER" />
        <result column="username"               property="username"             jdbcType="VARCHAR" />
        <result column="password"               property="password"             jdbcType="VARCHAR" />
    </resultMap>

    <sql id="Base_Column_List">
        id, username, password
    </sql>

    <insert id="insert" parameterType="com.junmeng.model.alpha.persistence.sigma.User" useGeneratedKeys="true" keyProperty="id">
        insert into exception_product
        (<include refid="Base_Column_List" />)
        value
        (#{id}, #{username}, #{password})
    </insert>
</mapper>